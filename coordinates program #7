//activity #7,


input.onButtonPressed(Button.A, function () {
    for (; ;) {
        let count = Math.randomRange(0, 25);
        for (let i = 0; i < count; i++) {
            led.toggle(Math.randomRange(0, 100), Math.randomRange(0, 100))
        }


    }
})

input.onButtonPressed(Button.B, function () {
    led.toggle(1, 1)
    led.toggle(3, 1)
    led.toggle(4,4)
    led.toggle(0,4)
    led.toggle(1,4)
    led.toggle(2, 4)
    led.toggle(3,4)
    led.toggle(0,3)
    led.toggle(4,3)
})
